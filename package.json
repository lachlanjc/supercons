{
  "name": "supercons",
  "version": "0.1.0",
  "description": "A friendly React iconset",
  "type": "module",
  "sideEffects": false,
  "source": "./src/index.tsx",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "keywords": [
    "icon",
    "react"
  ],
  "author": "Lachlan Campbell (https://lachlanjc.com) <lachlanjc@hey.com>",
  "scripts": {
    "dev": "yarn run prepare && yarn link && cd docs && yarn link supercons && next",
    "build:svg": "node scripts/generate-svg.mjs --experimental-modules",
    "prepare": "microbundle --jsx React.createElement",
    "watch": "microbundle watch --jsx React.createElement"
  },
  "peerDependencies": {
    "react": "^16.12.0 || ^17.0.1 || ^18.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lachlanjc/supercons.git"
  },
  "bugs": {
    "url": "https://github.com/lachlanjc/supercons/issues"
  },
  "homepage": "https://supercons.vercel.app"
}
