{
  "name": "supercons",
  "version": "0.1.4",
  "description": "A friendly React iconset",
  "type": "module",
  "sideEffects": false,
  "source": "./src/index.tsx",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "icon",
    "react"
  ],
  "author": "Lachlan Campbell (https://lachlanjc.com) <lachlanjc@hey.com>",
  "scripts": {
    "build:svg": "node scripts/generate-svg.mjs",
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react"
  },
  "peerDependencies": {
    "react": "^16.12.0 || ^17.0.1 || ^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lachlanjc/supercons.git"
  },
  "bugs": {
    "url": "https://github.com/lachlanjc/supercons/issues"
  },
  "homepage": "https://supercons.vercel.app",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "printWidth": 80,
    "semi": false
  },
  "dependencies": {
    "supercons": "^0.1.0"
  }
}
