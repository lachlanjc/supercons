{
  "name": "supercons",
  "version": "0.0.1",
  "description": "A friendly React iconset",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "icon",
    "icons",
    "svg",
    "react"
  ],
  "author": "Lachlan Campbell (https://lachlanjc.com) <lachlanjc@hey.com>",
  "scripts": {
    "dev": "yarn run prepare && yarn link && cd docs && yarn link supercons && next",
    "generate": "tsc {generate.ts,src/glyphs.tsx} --jsx react-jsx && node generate.js",
    "prepare": "rm -rf dist && yarn run generate && microbundle --jsx React.createElement",
    "watch": "yarn run prepare && microbundle watch -jsx React.createElement --no-compress",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "react": "^16.12.0 || ^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.24",
    "@types/react": "^17.0.1",
    "jsx-to-string": "^1.4.0",
    "microbundle": "^0.13.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lachlanjc/supercons.git"
  },
  "bugs": {
    "url": "https://github.com/lachlanjc/supercons/issues"
  },
  "homepage": "https://supercons.vercel.app"
}
